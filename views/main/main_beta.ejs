		.splitlayout {
			position: relative;
			/*overflow: auto;*/
			overflow-x: hidden;
			overflow-y: hidden;
			height: 100%;
			width: 100%;
		}

		/* Intro sides */
		.side {
			position: absolute;
			top: 0;
			z-index: 100;
			width: 50%;
			height: 100%;
			text-align: center;
			-webkit-backface-visibility: hidden;
		}

		.open-left .side,
		.open-right .side {
			cursor: default;
		}

		.overlay {
			position: absolute;
			top: 0;
			left: 0;
			z-index: 499;
			visibility: hidden;
			width: 100%;
			height: 100%;
			opacity: 0;
		}

		.side-left .overlay {
			background: rgba(0,0,0,0.7);
		}

		.side-right .overlay {
			background: rgba(0,0,0,0.3);
		}

		.side-left {
			left: 0;
			background: #e91e63;
			color: #00bcd4;
			outline: 1px solid #e91e63; /* avoid gap */
		}

		.side-right {
			right: 0;
			background: #00bcd4;
			color: #e91e63;
			outline: 1px solid #00bcd4; /* avoid gap */
		}

		/* Intro content, profile image and name, back button */
		.intro-content {
			position: absolute;
			/*top: 50%;
			left: 50%;*/
			/*padding: 0 1vw;*/
			width: 100%;
			height: 100%;
			cursor: pointer;
			-webkit-transform: translateY(0%) translateX(0%);
			transform: translateY(0%) translateX(0%);
		}

		.intro-content h1 > span {
			display: block;
			white-space: nowrap;
		}

		.intro-content h1 > span:first-child {
			font-weight: 300;
			font-size: 2vw;
		}

		.intro-content h1 > span:nth-child(2) {
			position: relative;
			text-transform: uppercase;
			letter-spacing: 1px;
			font-size: 0.8vw;
		}

		.intro-content h1 > span:nth-child(2):before {
			position: absolute;
			top: 0;
			left: 25%;
			width: 50%;
			height: 2px;
			background: #00bcd4;
			content: '';
		}

		.side-right .intro-content h1 > span:nth-child(2):before {
			background: #e91e63;
		}
		.back-right{
			position: absolute;
			right: 5%;
			top: 0%;
			cursor:pointer;
			width: 5%;

		}
		.back-left{
			position: absolute;
			left: 5%;
			top: 0%;
			cursor:pointer;
			width: 5%;			
		}

		.splitlayout.open-right {
			background: #008596;
		}

		.splitlayout.open-left {
			background: #860f37;
		}

		/* Inner page content */
		.page-inner {
			padding: 2vw;
		}

		.page-inner section {
			padding-bottom: 1vw;
		}

		.page-inner h2 {
			margin: 0 0 1vw 0;
			font-weight: 300;
			font-size: 2.4vw;
		}

		.page-inner p {
			font-weight: 300;
			font-size: 1.2vw;
		}

		/* All transitions */
		.side,
		.page {
			-webkit-transition: -webkit-transform 0.6s;
			transition: transform 0.6s;
		}

		.overlay {
			-webkit-transition: opacity 0.6s, visibility 0.1s 0.6s;
			transition: opacity 0.6s, visibility 0.1s 0.6s;
		}

		.intro-content {
			-webkit-transition: -webkit-transform 0.6s, top 0.6s;
			transition: transform 0.6s, top 0.6s;
		}

		.intro-content h1,
		.back {
			-webkit-transition: opacity 0.3s;
			transition: opacity 0.3s;
		}

		/* Open and close */

		/* We need to set the position and overflow for the respective page scroll */
		
		.splitlayout.close-left {
			position: absolute;
			overflow: hidden;
			height: 100%;
			background: #860f37;
		}
		.splitlayout.close-right{
			position: absolute;
			overflow: hidden;
			height: 100%;
			background: #008596;
		}

		.open-right .side-left .overlay,
		.open-left .side-right .overlay {
			visibility: visible;
			opacity: 1;
			-webkit-transition: opacity 0.6s;
			transition: opacity 0.6s;
		}

		/* Right side open */
		.open-right .side-left {
			-webkit-transform: translateX(-60%);
			transform: translateX(-60%);
		}

		.open-right .side-right {
			z-index: 200;
			-webkit-transform: translateX(-150%);
			transform: translateX(-150%);
		}

		.close-right .side-right {
			z-index: 200;
		}

		.open-right .side-right .intro-content {
			-webkit-transform: translateY(0%) translateX(25%) scale(0.6);
			transform: translateY(0%) translateX(25%) scale(0.6);
		}

		/* Left side open */
		.open-left .side-right {
			-webkit-transform: translateX(60%);
			transform: translateX(60%);
		}

		.open-left .side-left {
			z-index: 200;
			-webkit-transform: translateX(150%);
			transform: translateX(150%);
		}

		.close-left .side-left {
			z-index: 200;
		}

		.open-left .side-left .intro-content {
			-webkit-transform: translateY(0%) translateX(-25%) scale(0.6);
			transform: translateY(0%) translateX(-25%) scale(0.6);
		}
